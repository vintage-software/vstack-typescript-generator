{
  "name": "vstack-typescript-generator",
  "version": "0.1.2",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "clean": "rimraf dist & rimraf bundles",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "npm run clean & tsc -p tsconfig.json & tsc -p tsconfig-spec.json",
    "test": "istanbul cover node_modules/jasmine/bin/jasmine.js && npm run remap-coverage",
    "remap-coverage": "npm run remap-coverage-json & npm run remap-coverage-html",
    "remap-coverage-json": "remap-istanbul -i coverage/coverage.json -o coverage/coverage-remapped.json",
    "remap-coverage-html": "remap-istanbul -i coverage/coverage-remapped.json -o coverage/lcov-report-remapped -t html",
    "report-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/vstack-typescript-generator.git"
  },
  "author": "Vintage Software",
  "license": "MIT",
  "collaborators": [
    {
      "name": "Kevin Phelps",
      "email": "kphelps@vintagesoftware.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/vintage-software/vstack-typescript-generator/issues"
  },
  "homepage": "https://github.com/vintage-software/vstack-typescript-generator#readme",
  "dependencies": {
    "pluralize": "1.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.2.31",
    "codecov": "1.0.1",
    "istanbul": "0.4.4",
    "jasmine": "2.4.1",
    "remap-istanbul": "0.6.4",
    "rimraf": "2.5.4",
    "tslint": "3.7.4",
    "typescript": "^2.0.0"
  }
}
