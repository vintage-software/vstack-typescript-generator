{
  "name": "vstack-typescript-generator",
  "version": "0.8.3",
  "description": "Tool to generate TypeScript interfaces and TypeScript filter classes for Vstack from C# sources.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tsgen": "./dist/tsgen-cli.js"
  },
  "repository": "https://github.com/vintage-software/vstack-typescript-generator.git",
  "author": "Vintage Software",
  "contributors": [
    "Kevin Phelps <kphelps@vintagesoftware.com> (https://kevinphelps.me/)"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "ts-node ./build/lint.ts",
    "build": "ts-node ./build/build.ts",
    "test": "ts-node ./build/test.ts",
    "print-coverage": "istanbul report -t text",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@types/camelcase": "4.1.0",
    "@types/globby": "6.1.0",
    "@types/pluralize": "0.0.28",
    "camelcase": "4.1.0",
    "chalk": "2.3.0",
    "globby": "7.1.1",
    "mkdirp": "0.5.1",
    "pluralize": "7.0.0",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "@types/camelcase": "4.1.0",
    "@types/diff": "3.2.2",
    "@types/globby": "6.1.0",
    "@types/jasmine": "2.8.3",
    "@types/mkdirp": "0.5.2",
    "@types/node": "8.5.7",
    "@types/pluralize": "0.0.28",
    "@types/rimraf": "2.0.2",
    "@types/yargs": "10.0.1",
    "codecov": "3.0.0",
    "codecov.io": "0.1.6",
    "diff": "3.4.0",
    "dir-compare": "1.4.0",
    "istanbul": "0.4.5",
    "jasmine": "2.8.0",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "tslint": "5.8.0",
    "tslint-consistent-codestyle": "1.11.0",
    "typescript": "2.6.2"
  }
}
