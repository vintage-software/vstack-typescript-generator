{
  "name": "vstack-typescript-generator",
  "version": "0.6.2",
  "description": "Tool to generate TypeScript interfaces and TypeScript filter classes for Vstack from C# sources.",
  "main": "dist/src/tsgen.js",
  "bin": {
    "tsgen": "dist/src/tsgen-cli.js"
  },
  "scripts": {
    "patch-release": "npm test && npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm test && npm version minor && npm publish && git push --follow-tags",
    "clean": "rimraf dist & rimraf coverage",
    "lint": "tslint --project tsconfig.json --type-check --fix",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc --project tsconfig.json",
    "pretest": "npm run build",
    "test": "npm run test:spec && npm run test:generate && npm run test:compare",
    "test:spec": "istanbul cover node_modules/jasmine/bin/jasmine.js",
    "test:generate": "rimraf test-project/test-generated && node dist/src/tsgen-cli.js test-project/tsgen.json",
    "test:compare": "dircompare -c test-project/generated test-project/test-generated",
    "posttest": "remap-istanbul -i coverage/coverage.json -o coverage -t html",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/vstack-typescript-generator.git"
  },
  "bugs": {
    "url": "https://github.com/vintage-software/vstack-typescript-generator/issues"
  },
  "author": "Vintage Software",
  "contributors": [
    "Kevin Phelps <kphelps@vintagesoftware.com> (https://kevinphelps.me/)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vintage-software/vstack-typescript-generator#readme",
  "dependencies": {
    "camelcase": "4.1.0",
    "globby": "6.1.0",
    "mkdirp": "0.5.1",
    "pluralize": "7.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.6.0",
    "@types/mkdirp": "0.5.1",
    "@types/node": "8.0.45",
    "@types/rimraf": "2.0.2",
    "codecov.io": "0.1.6",
    "dir-compare": "1.4.0",
    "istanbul": "0.4.5",
    "jasmine": "2.8.0",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.2",
    "tslint": "5.7.0",
    "typescript": "2.5.3"
  }
}
