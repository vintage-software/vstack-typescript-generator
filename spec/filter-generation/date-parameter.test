export class PeopleByBirthdatePrimaryFilter extends PrimaryFilter<Person> {
  constructor(private birthdate: Date, private birthdates: Date[]) {
    super();
  }

  public getFilterName(): string {
    return 'ByBirthdate';
  }

  public getParameters(): string[] {
    return [encodeURIComponent(this.birthdate === null || this.birthdate === undefined ? this.birthdate ? this.birthdate.toISOString()), this.birthdates.map(value => encodeURIComponent(value === null || value === undefined ? value ? value.toISOString())).join(',')];
  }

  protected __dummy(): Person {
    return null;
  }
}